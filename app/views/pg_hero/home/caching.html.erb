<div class="content">
  <h1>Caching</h1>

  <p>Tables</p>

  <table class="table">
    <thead>
      <tr>
        <th>Table</th>
        <th style="width: 20%;">Hit Rate</th>
      </tr>
    </thead>
    <tbody>
      <% @table_caching.each do |table| %>
        <tr>
          <td><%= table["table"] %></td>
          <td>
            <%= (table["hit_rate"].to_f * 100).round %>%
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <p>Indexes</p>

  <table class="table">
    <thead>
      <tr>
        <th>Index</th>
        <th style="width: 20%;">Hit Rate</th>
      </tr>
    </thead>
    <tbody>
      <% @index_caching.each do |table| %>
        <tr>
          <td><%= table["index"] %> <span class="text-muted"><%= table["table"] %></span></td>
          <td>
            <%= (table["hit_rate"].to_f * 100).round %>%
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
